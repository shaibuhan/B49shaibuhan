class Person {
  constructor(name) {
    this.fullName = name;
  }
  
  getName() {
    return this.fullName;
  }
}
class Employee extends Person {
  constructor(name, id){
    super(name);
    this.id = id;
  }
  getId(){
    return this.id
  }
}
let obj = new Employee("john", "001");
console.log(obj.getName(), obj.getId());
